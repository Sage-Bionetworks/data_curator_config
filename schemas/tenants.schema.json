{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "tenants": {
      "type": "object",
      "required": [ "tenant" ],
      "additionalProperties": false,
      "properties": {
        "tenant": {
          "type": "object",
          "required": [ "name", "synapse_asset_view", "config_location" ],
          "additionalProperties": false,
          "properties": {
            "name": {
              "type": "string"
            },
            "synapse_asset_view": {
              "type": "string",
              "pattern": "^syn[0-9]+$"
            },
            "config_location": {
              "type": "string",
              "pattern": "^(dcc|test).+\\.json$"
            }
          }
        }
      }
    }
  }
}
